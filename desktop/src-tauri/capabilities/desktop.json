{
  "identifier": "desktop-only",
  "description": "Desktop-only capabilities (menu, tray, autostart, window state)",
  "platforms": ["linux", "macOS", "windows"],
  "windows": ["main"],
  "permissions": [
    "core:menu:default",
    "autostart:default",
    "autostart:allow-enable",
    "autostart:allow-disable",
    "autostart:allow-is-enabled"
  ]
}
